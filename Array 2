package vijesh;

public class array2 {

    public static void main(String[] args) {
        String[] words = {"abc", "bcd", "aaaa", "cbc"};
        int[] arr = new int[words.length]; 
        int count = 0; 
        for (int i = 0; i < words.length; i++) {
            if (words[i].contains("a")) {
                arr[count] = i;
                count++;
            }
        }
        for (int i = 0; i <count; i++) {
            System.out.print(arr[i] + " ");
        }
    }
}
